{
  "name": "temporary_file",
  "version": "0.1.0",
  "author": "Lawrence Kok",
  "summary": "Manages temporary files with automatic cleanup after Puppet stages.",
  "license": "proprietary",
  "source": "https://github.com/phr34k/puppet_temporary_file",
  "project_page": "https://github.com/phr34k/puppet_temporary_file",
  "issues_url": "https://github.com/phr34k/puppet_temporary_file/issues",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0 < 10.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [ "10", "11", "12" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "18.04", "20.04", "22.04" ]
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [ "7", "8", "9" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [ "7", "8" ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [ "36", "37", "38", "39", "40" ]
    },
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [ "Server 2012 R2", "Server 2016", "Server 2019", "Server 2022" ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 6.0.0 < 9.0.0"
    }
  ],
  "pdk_version": "3.1.0"
}